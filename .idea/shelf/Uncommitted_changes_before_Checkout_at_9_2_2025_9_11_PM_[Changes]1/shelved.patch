-                        form.setFieldValue("description", e.currentTarget.value);
-                    }}
-                    rightSection={
-                        <Text size="xs" c="dimmed">
-                            {desc.length}/{maxDescLength}
-                        </Text>
-                    }
-                    rightSectionWidth={80}
-                    autosize
-                    minRows={3}
-                    maxLength={maxDescLength}
-                    mb="md"
-                />
-
-                {/* Danh sách cần lặp */}
